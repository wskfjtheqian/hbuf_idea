<templateSet group="Hbuf">
    <template description='Import declaration' name='imp' toReformat='true' toShortenFQNames='true'
              value='import ""$END$'>
        <context>
            <option name='HBUF' value='true'/>
        </context>
    </template>
    <template description='Package declaration' name='p' toReformat='true' toShortenFQNames='true'
              value='package $KEY$ = "$NAME$"$END$'>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='KEY'/>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='NAME'/>
        <context>
            <option name='HBUF' value='true'/>
        </context>
    </template>
    <template description='Enum declaration' name='e' toReformat='true' toShortenFQNames='true'
              value='enum $NAME$ {$END$}$END$'>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='NAME'/>
        <context>
            <option name='HBUF' value='true'/>
        </context>
    </template>
    <template description='Data declaration' name='d' toReformat='true' toShortenFQNames='true'
              value='data $NAME$ = $ID$ {$END$}$END$'>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='NAME'/>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='$ID$'/>
        <context>
            <option name='HBUF' value='true'/>
        </context>
    </template>
    <template description='Server declaration' name='s' toReformat='true' toShortenFQNames='true'
              value='server $NAME$ = $ID$ {$END$}$END$'>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='NAME'/>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='$ID$'/>
        <context>
            <option name='HBUF' value='true'/>
        </context>
    </template>
    <template description='Server function declaration' name='f' toReformat='true' toShortenFQNames='true'
              value='$NAME$Reps $NAME$($NAME$Req req) = $ID$ $END$'>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='NAME'/>
        <variable alwaysStopAt='true' defaultValue='' expression='complete()' name='$ID$'/>
        <context>
            <option name='HBUF' value='true'/>
        </context>
    </template>


